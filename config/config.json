{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "connect-me-xero-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/connectMeXero/ConnectMeXeroApplicationCustomizer.js",
          "manifest": "./src/extensions/connectMeXero/ConnectMeXeroApplicationCustomizer.manifest.json"
        },
        {
          "entrypoint": "./lib/extensions/connectMeServiceNow/ConnectMeServiceNowApplicationCustomizer.js",
          "manifest": "./src/extensions/connectMeServiceNow/ConnectMeServiceNowApplicationCustomizer.manifest.json"
        },
        {
          "entrypoint": "./lib/extensions/connectMeZendesk/ConnectMeZendeskApplicationCustomizer.js",
          "manifest": "./src/extensions/connectMeZendesk/ConnectMeZendeskApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ConnectMeXeroApplicationCustomizerStrings": "lib/extensions/connectMeXero/loc/{locale}.js",
    "ConnectMeServiceNowApplicationCustomizerStrings": "lib/extensions/connectMeServiceNow/loc/{locale}.js",
    "ConnectMeZendeskApplicationCustomizerStrings": "lib/extensions/connectMeZendesk/loc/{locale}.js"
  }
}
